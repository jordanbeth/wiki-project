<h1 class="login-title">Login</h1>
<form class="login-form" action="/login" method="post">
  <input class="login-form" type="username" name="username" placeholder="Username"><br>
  <input class="login-form" type="password" name="password" placeholder="Password"><br>
  <input class="login-form" type="submit" name="Submit" value="Login">
</form>

<%=erb :"_errors"%>
